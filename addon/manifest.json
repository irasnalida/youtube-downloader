{
  "description": "Youtube Downloader addon",
  "manifest_version": 2,
  "name": "YouTube Downloader",
  "version": "0.0.3",
  "icons": {
    "48": "icons/icon.svg"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "sytd_host@irasnalida"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/icon.svg"
  },
  "permissions": [
    "*://www.youtube.com/*",
    "contextMenus",
    "activeTab",
    "nativeMessaging",
    "cookies",
    "tabs"
  ]
}
